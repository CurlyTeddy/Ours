// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  userId    String   @id @default(cuid()) @map("user_id")
  email     String   @unique
  name      String
  password  String

  @@map("user")
}
